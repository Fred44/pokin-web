{
  "hosting": {
    "public": "dist/pokin-dev",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css|svg)",
        "headers": [ {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        } ]
      }
    ]
  }
}
